      <add key="EmailServiceUrl" value="http://192.168.100.123:8290/mailserv/send-mail"/>
      <add key="chunksize" value="15"/>
      <add key="UseProxy" value="false" />
      <add key="ProxyIP" value="10.10.21.22" />
