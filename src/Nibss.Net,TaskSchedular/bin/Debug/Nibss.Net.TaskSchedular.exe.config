      <add key="Flag" value="3"/>
      <add key="InfoBeepServiceUrl" value="https://oneapi.infobip.com/" />
      <add key="InfoBeepUserName" value="nibss-plc" />
      <add key="InfoBeepPassword" value="Nibss@1230"/>
